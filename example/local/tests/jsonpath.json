{
  "name": "jsonpath",
  "threadSpecs": {
    "var_demo": {
      "variableDefs": {
        "my_int_var": {
          "type": "INT",
          "required": false
        },
        "my_json_blob": {
          "type": "JSON_OBJ",
          "required": false,
          "defaultVal": {
            "type": "JSON_OBJ",
            "jsonObj": "{\"foo\": \"bar\", \"asdf\": {\"my_int\": 5}}"
          }
        }
      },
      "nodes": {
        "entrypoint": {
          "outgoingEdges": [
            {
              "sinkNodeName": "t1"
            }
          ],
          "entrypoint": {}
        },
        "t1": {
          "task": {
            "taskDefName": "task1",
            "retries": 3
          },
          "variableMutations": [{
            "lhsName": "my_int_var",
            "operation": "ASSIGN",
            "sourceVariable": {
              "variableName": "my_json_blob",
              "jsonPath": ".asdf.my_int"
            }
          }],
          "outgoingEdges": [{
            "sinkNodeName": "exit"
          }]
        },
        "exit": {
          "exit": {}
        }
      }
    }
  },
  "entrypointThreadName": "var_demo"
}
